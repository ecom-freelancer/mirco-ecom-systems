{
  "compilerOptions": {
    "target": "es2017",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "declaration": true,
    "declarationMap": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "preserveConstEnums": true,
    "skipLibCheck": true,
    "noEmitOnError": true,
    "importHelpers": true,
    "removeComments": true,
    "noUnusedLocals": true,
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": true,
    "allowJs": true
  }
}
